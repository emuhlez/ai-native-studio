{
  "name": "ai-native-studio",
  "version": "0.1.0",
  "description": "AI-native game editor built with React and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsx --watch server/index.ts\"",
    "dev:client": "vite",
    "dev:server": "tsx --watch server/index.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.45",
    "@ai-sdk/react": "^3.0.91",
    "ai": "^6.0.90",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "lucide-react": "^0.294.0",
    "nanoid": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.182.0",
    "uuid": "^9.0.1",
    "zod": "^4.3.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.182.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
